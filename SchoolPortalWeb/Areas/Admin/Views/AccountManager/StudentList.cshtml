@model List<ApplicationUser>

<h3 class="text-center">User List </h3>
<div class="text-end">
    <a class="col-2 btn btn-primary mb-3" asp-area="Student" asp-controller="Home" asp-action="Privacy">View Admin/Teachers</a>
  @*   <a class="col-2 btn btn-primary mb-3" asp-area="Admin" asp-controller="AccountManager" asp-action="Create">+ Add Student(s)</a> *@
    <a class="col-2 btn btn-primary mb-3" id="register" asp-area="Identity" asp-page="/Account/Register">Add Student</a>
</div>
<table class="table table-striped text-center">
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Role</th>
        <th scope="col">Username</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Email</th>
        <th scope="col">Year Enrolled</th>
        <th scope="col">Actions</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.Role</td>
            <td>@user.UserName</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>@user.YearEnrolled</td>
            <td>
                <span class="pe-2">Grades</span>
                <span class="pe-2">Edit</span>
                Delete
            </td>
        </tr>
    }
</table>
